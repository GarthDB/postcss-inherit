{
  "name": "postcss-inherit",
  "description": "PostCSS plugin inherit rules from other selectors",
  "main": "./lib/index.js",
  "scripts": {
    "start": "package-scripts",
    "test": "package-scripts test",
    "prepublish": "package-scripts build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/postcss-inherit.git"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "inherit",
    "extend"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/garthdb/postcss-inherit/issues"
  },
  "homepage": "https://github.com/topmarks/topmarks#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "postcss": "^5.0.10",
    "postcss-inherit-parser": "^0.1.3"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "codeclimate-test-reporter": "^0.3.3",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-customizable": "^4.0.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "nyc": "^8.1.0",
    "p-s": "^2.4.3",
    "perfectionist": "^2.1.4",
    "postcss-import": "^8.1.2",
    "semantic-release": "^4.3.5",
    "topcoat-utils": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
